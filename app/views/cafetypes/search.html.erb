<%= form_with(url: search_cafetypes_path, local: true, method: :get, class: "search-form") do |form| %>
  <%= form.text_field :keyword, placeholder: "投稿を検索する", class: "search-input" %>
  <%= form.submit "検索", class: "search-btn" %>
<% end %>
<div class="card__wrapper">
  <% @cafetypes.each do |cafetype| %>
    <%= render partial: "cafetype", locals: { cafetype: cafetype } %>
  <% end %>
</div>