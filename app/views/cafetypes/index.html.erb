<main class="main">
  <div class="inner">
   
    <% if user_signed_in? %>
      <div class="greeting">
       <p>こんにちは、
        <%= link_to "#{current_user.nickname}さん" , user_path(current_user), class: :greeting__link %>
       </p> 
      </div>
      <% else %>
      <% end %>
   
    <div class="card__wrapper">
    
      <%= render partial: 'cafetype', collection: @cafetypes %>
    </div>
  </div>
</main>
